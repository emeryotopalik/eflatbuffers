enum Color : byte { Red, Green, Blue }

struct Nest {
  age:long;
  color:Color;
  shortNum:short;
}

struct AnotherNest {
  color:Color;
}

struct Everything {
  integer:int;
  nested:Nest;
  anotherNest:AnotherNest;
  size:float;
  color:Color;
  shortNum:short;
}

table MyTable {
  everything:Everything;
}

root_type MyTable;